{
  "name": "me",
  "version": "0.2.2",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate --fail-on-error",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,sass,scss,vue}\" --ignore-path .gitignore",
    "prepare": "husky install && node js-beautify-wa.js",
    "lint": "yarn lint:js && yarn lint:style",
    "test": "jest",
    "eslint": "eslint '*/**/*.{js,jsx,ts,tsx,vue}' --fix",
    "prettier": "prettier --config .prettierrc.js '**/*.{json,js,jsx,ts,tsx,css,scss,md,vue}' --write",
    "jsbeautify": "js-beautify",
    "stylelint": "stylelint '**/*.{css,sass,scss,vue}' --fix"
  },
  "lint-staged": {
    "*.{css,sass,scss}": [
      "stylelint --fix",
      "js-beautify --css -r",
      "prettier --write"
    ],
    "*.vue": [
      "jest --bail --findRelatedTests",
      "stylelint --fix",
      "js-beautify --config ./.jsbeautifyrc.json --html -r",
      "prettier --write",
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx}": [
      "jest --bail --findRelatedTests",
      "prettier --write",
      "eslint --fix"
    ],
    "*.json": "prettier --write"
  },
  "dependencies": {
    "core-js": "^3.21.1",
    "nuxt": "^2.15.7",
    "vue": "^2.6.14",
    "vuetify": "^2.6.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@nuxtjs/eslint-config": "^9.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/vuetify": "^1.12.3",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.5.1",
    "eslint": "^8.11.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "js-beautify": "^1.14.0",
    "lint-staged": "^12.3.5",
    "node-sass": "^7.0.1",
    "prettier": "^2.5.1",
    "replace-in-file": "^6.3.2",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^4.1.0",
    "vue-jest": "^3.0.4"
  }
}
